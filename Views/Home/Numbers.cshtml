@model int[]

@{
    ViewData["Title"] = "Home Page";
}

<main class="container">
    <h1 class="display-4 text-center mb-5">Here are some numbers!</h1>
    <div class="row">
      <div class="col-2 border mx-auto text-center">
        @{
          foreach (int num in Model)
          {
            <p>@num</p>
          }
        }
      </div>
    </div>
</main>
