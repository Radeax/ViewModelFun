@using ViewModelFun.Models
@model User[]

@{
    ViewData["Title"] = "Home Page";
}

<main class="container">
    <h1 class="display-4 text-center mb-5">Here are some Users!</h1>
    <div class="row">
      <div class="col-2 border mx-auto text-center">
        @{
          foreach (User user in Model)
          {
            <p>@user.Name</p>
          }
        }
      </div>
    </div>
</main>
